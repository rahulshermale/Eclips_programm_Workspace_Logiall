package pack_01;

public class Binary_NUmber {

	public static void main(String[] args) {

		int row = 10;
		int col = 10;
		for (int i = 1; i <= row; i++) {

			for (int j = 1; j <= col; j++) {

				if (i == j||i>=j) {

					System.out.print("* ");
				}
				else if (i < j&&i>=j) 
				{
					System.out.print(" ");
				}
				
				
			}
			System.out.println();
		}

	}
}