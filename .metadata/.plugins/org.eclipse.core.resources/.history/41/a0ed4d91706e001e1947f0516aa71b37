package mypack1;

class Demo{
	
	static int num1=20;

	public int Calculation(int num) {
		
		int sum= num * num1;
		return sum;
	}

	
	
	
}

public class Sum {

	public static void main(String[] args) {
		Demo d=new Demo();
		int num = 10;  //local variable
		
		
		int abc = d.Calculation(num);
		
		
		
		System.out.println(abc);
	}
}
