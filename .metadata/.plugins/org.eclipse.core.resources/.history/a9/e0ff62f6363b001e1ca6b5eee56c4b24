import java.util.Scanner;

public class Question_02 {
public static void main(String[] args) {
	
//	Scanner sc=new Scanner(System.in);
//	System.out.println("Enter Number");
//	
//	int num=sc.nextInt();
	int a=1,b=1;
	for(int i=1;i<=3;i++) {
		for(int j=1;j<=i;j++) {
			System.out.print(a+" ");
			int c=a+b;
			a=b;
			b=c;
			
		}
		System.out.println();
	}
}
}
