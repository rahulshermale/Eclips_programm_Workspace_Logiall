package pack_01;

public class Binary_NUmber {

	public static void main(String[] args) {

		int row = 10;
		int col = 10;
		int lcol=10;
		for (int i = 1; i <= row; i++) {

			for (int j = 1; j <= col; j++) {

				if(row==1||col==1||col==5||row==4 ) {

					System.out.print("* ");
				}
				else 
				{
					System.out.print(" *");
				}
				
				
			}
			System.out.println();
		}

	}
}